v010A37E0_0 .var "temp", 1 0;
TD_decoder_tb.decoder.dec ;
    %load/v 8, v010A37E0_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_0.0, 4;
    %movi 8, 1, 4;
    %set/v v010A47B0_0, 8, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v010A37E0_0, 2;
    %cmpi/u 8, 1, 2;
    %jmp/0xz  T_0.2, 4;
    %movi 8, 2, 4;
    %set/v v010A47B0_0, 8, 4;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v010A37E0_0, 2;
    %cmpi/u 8, 2, 2;
    %jmp/0xz  T_0.4, 4;
    %movi 8, 4, 4;
    %set/v v010A47B0_0, 8, 4;
    %jmp T_0.5;
T_0.4 ;
    %movi 8, 8, 4;
    %set/v v010A47B0_0, 8, 4;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %end;
    .scope S_00F93ED8;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "decoder_tb.vcd";
    %vpi_call 2 12 "$dumpvars", 1'sb0, S_00F93ED8;
